[ZEN]
// Each Monster Money Rate is Set in Monster.txt (NOTE: in Monster.txt The Higher The Rate The Lower The Chance to Drop)
DropRate		= 10	                ; Zen Global Drop Rate (Default: 10)
; Zen Multiplier Value For Normal Class
ZenMultiplierNormal	= 1.000                 
; Zen Multiplier Value For Master Class
ZenMultiplierMaster	= 1.000                 
MaxVaultZen		= 100000000

[NOTICE]
EnableNoticeSystem	= 1                     ; News System [Data/NoticeList.txt] (0 = Disabled / 1 = Enabled)
SlideNoticeMode		= 1                     ; Show News in Slide Mode (0 = Disabled / 1 = Enabled)
ColorRedValue		= 100 	                ; News Red Color Value (Range: 0-255)
ColorGreenValue		= 100 	                ; News Green Color Value (Range: 0-255)
ColorBlueValue		= 0 	                ; News Blue Color Value (Range: 0-255)


[RANKING]
AnnounceTopPk		= 1                     ; Announce TOP PK Player InGame (0 = Disabled / 1 = Enabled)
AnnounceTopRr		= 1                     ; Announce TOP Reset Player InGame (0 = Disabled / 1 = Enabled)
ResetTopPkRank		= 2	                ; Reset TOP PK Ranking (1 = Daily, 2 = Weekly (Sunday), 3 = Monthly, 0 = Disabled)
TopPkRewardPcPoints	= 0			; this value will be set to TemporaryPcPointColumn in eds.ini
TopPkRewardCsPoints	= 0			; this value will be set to TemporaryCreditColumn in eds.ini


[EXTENDEDVAULT]
EnableExtraVault	= 0                     ; Extra Vault System (0 = Disabled / 1 = Enabled)
MaxExtraVaults		= 10                    ; Maximum Extra Vaults Per Account

EnableGuildVault	= 0                     ; Guild Vault System (0 = Disabled / 1 = Enabled)
MaxGuildVaults		= 5                     ; Maximum Vaults Per Guild
GuildVaultNpcId		= 240                   ; NPC ID For Guild Vault
VaultOpenDelay		= 5                     ; [Anti-Flood] Vault Open Delay (Seconds)


[PERSONALSTORE]
// Set Credits' Table & Column Accordingly in eDS/eDS.ini (SQLADVANCE Section)
BuySellItemUsingCredits		= 0		; Personal Shop Currency (0 = Use Zen / 1 = Use Credits)
AccountCreditPointsLimit	= 2000000000	; Maximum Credits Limit Per Account
BuySellCreditsTax		= 0		; 0 to 100, x rate to reduce from the sold amount. (credits to reduce = x * amount / 100)


[PKPENALTY]
MinPkLevelNoExp		= 5	                ; Minimum PK Level That Can Receive Experience (0 = Disabled)
MaxPkLevelCanBuy	= 4	                ; Maximum PK Level That Can Buy in Shops (0 = Disabled)
// Need Client Patch
MaxPkLevelCanWarp	= 4	                ; Maximum PK Level That Can Warp (0 = Disabled)


[EVENTDROP]
// 1 - Exact Number Of Exc Options (That You Set in MaxExc Column Of EventItemBag)
// 0 - Random Number Of Exc Options (From 1 to MaxExc Column Value Of EventItemBag)
KundunMaxExcFixed	= 1                     ; Kundun Exc Option Drop Type
SeniorMaxExcFixed	= 1                     ; Seniox Exc Option Drop Type
ErohimMaxExcFixed	= 1                     ; Erohim Exc Option Drop Type

KundunMaxDrop		= 3                     ; Kundun Maximum Items Drop
KundunAncientDropRate	= 2500	                ; Kundun Ancient Drop Rate = X / 10000 (Default: 2500)


[NPC]
ShadowPhantomBufPlusDuration	= 0	        ; Additional Buff Duration From Elf Buffer NPC (Seconds)
; Attack Buff Multiplier
ShadowPhantomBufAttackMulti	= 0.000
; Defense Buff Multiplier         
ShadowPhantomBufDefenceMulti	= 0.000 
MarlonNoAutoWarp		= 0		; 1 - marlon npc will stay in the location you set in monstersetbase        

